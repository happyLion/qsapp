<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
<include layout="@layout/main_layout" />
<LinearLayout 
	android:orientation="vertical" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent">
	<include layout="@layout/main_top" />
		<LinearLayout
			android:id="@+id/zong"
			android:layout_width="fill_parent" 
			android:layout_height="0dp"
			android:layout_weight="1"
			android:orientation="vertical">
			<include layout="@layout/config_custom"
				android:id="@+id/include" />

		<LinearLayout
			android:id="@+id/grid_zong"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"
			>
			<LinearLayout
				android:id="@+id/up"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:orientation="horizontal"
				>
				<RelativeLayout
					android:id="@+id/up_left_image_and_text"
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:orientation="vertical"
					android:layout_weight="1"
					>

					<ImageView
						android:layout_width="60dp"
						android:layout_height="60dp"
						android:id="@+id/imageView_up_left"
						android:layout_centerInParent="true"
						android:src="@drawable/helpdocumentcolor"
						android:padding="5dp"
						/>
					<TextView
						android:id="@+id/TextView_up_left"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_below="@id/imageView_up_left"
						android:layout_centerHorizontal="true"
						android:text="帮助文档"
						android:textSize="12sp"/>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/up_right_image_and_text"
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="60dp"
						android:layout_height="60dp"
						android:id="@+id/imageView_up_right"
						android:src="@drawable/signature"
						android:layout_centerInParent="true"
						android:padding="5dp"
						/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="修改签名"
						android:layout_below="@id/imageView_up_right"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"/>
				</RelativeLayout>
			</LinearLayout>

			<LinearLayout
				android:id="@+id/middle"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1"
				android:orientation="horizontal"

				>
				<RelativeLayout
					android:id="@+id/middle_left_image_and_text"
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:orientation="vertical"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="60dp"
						android:layout_height="60dp"
						android:id="@+id/imageView_middle_left"
						android:layout_centerInParent="true"
						android:padding="5dp"
						android:src="@drawable/recordvideo"
						/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="录制视频"
						android:layout_below="@id/imageView_middle_left"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"/>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/middle_right_image_and_text"
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:orientation="vertical"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="60dp"
						android:layout_height="60dp"
						android:id="@+id/imageView_middle_right"
						android:layout_centerInParent="true"
						android:padding="5dp"
						android:src="@drawable/changehead"
						/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="修改头像"
						android:layout_below="@id/imageView_middle_right"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"/>
				</RelativeLayout>
			</LinearLayout>

			<LinearLayout
				android:id="@+id/down"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1.5"
				android:orientation="vertical">

				<RelativeLayout
					android:id="@+id/down_left_image_and_text"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:id="@+id/imageView_down_left"
						android:layout_alignParentLeft="true"
						android:padding="10dp"
						android:src="@drawable/sensitive"
						/>
					<!--<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="灵敏度"
						android:layout_below="@id/imageView_down_left"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"/>  -->
						
					<SeekBar
					    android:id="@+id/seekbar_up"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_toRightOf="@+id/imageView_down_left"
							android:layout_centerVertical="true"
							android:layout_marginRight="10dp"
							android:max="100"
							android:progress="30"
							/>
					<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="20s"
					android:layout_alignLeft="@id/seekbar_up"
					android:layout_alignParentBottom="true"
					android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="100s"
						android:layout_alignRight="@id/seekbar_up"
						android:layout_alignParentBottom="true"
						android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="间隔度"
						android:layout_alignParentBottom="true"
						android:layout_alignParentLeft="true"
						android:textSize="10sp"
						android:layout_marginLeft="5dp"/>
				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/down_right_image_and_text"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:id="@+id/imageView_down_right"
						android:layout_alignParentLeft="true"
						android:padding="10dp"
						android:src="@drawable/sensitive"
						/>
					<!-- <TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="其他"
						android:layout_below="@id/imageView_down_right"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"/> -->
					<SeekBar
					    android:id="@+id/seekbar_middle"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_toRightOf="@+id/imageView_down_right"
							android:layout_centerVertical="true"
							android:layout_marginRight="10dp"
							android:max="100"
							android:progress="30"
							/>
					<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="0"
					android:layout_alignLeft="@id/seekbar_middle"
					android:layout_alignParentBottom="true"
					android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="100"
						android:layout_alignRight="@id/seekbar_middle"
						android:layout_alignParentBottom="true"
						android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="负载度"
						android:layout_alignParentBottom="true"
						android:layout_alignParentLeft="true"
						android:textSize="10sp"
						android:layout_marginLeft="5dp"/>
						
				</RelativeLayout>
				
				<RelativeLayout
					android:id="@+id/down_left_image_and_text_add"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="1"
					>
					<ImageView
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:id="@+id/imageView_down_left_add"
						android:layout_alignParentLeft="true"
						android:padding="10dp"
						android:src="@drawable/sensitive"
						/>
					<!--<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="开开心心"
						android:layout_below="@id/imageView_down_left"
						android:layout_centerHorizontal="true"
						android:textSize="12sp"
						android:id="@+id/textView" />-->
					<SeekBar
						android:id="@+id/seekbar_down"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_toRightOf="@+id/imageView_down_left_add"
						android:layout_centerVertical="true"
						android:layout_marginRight="10dp"
						android:max="100"
						android:progress="30"
						/>
					<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="0"
					android:layout_alignLeft="@id/seekbar_down"
					android:layout_alignParentBottom="true"
					android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="100"
						android:layout_alignRight="@id/seekbar_down"
						android:layout_alignParentBottom="true"
						android:textSize="10sp"/>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="灵敏度"
						android:layout_alignParentBottom="true"
						android:layout_alignParentLeft="true"
						android:textSize="10sp"
						android:layout_marginLeft="5dp"/>

				</RelativeLayout>
			</LinearLayout>

		</LinearLayout>

		</LinearLayout>
	<include layout="@layout/main_tab" />
</LinearLayout>
</merge>